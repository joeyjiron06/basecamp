{
  "body": {
    "@apply text-foreground-700 text-base leading-loose": {},
    "> :first-child": {
      "@apply mt-0": {}
    },
    "> :last-child": {
      "@apply mb-0": {}
    }
  },
  "p": {
    "@apply mt-5 mb-5": {}
  },
  "[class~=\"lead\"]": {
    "@apply text-foreground-600 text-xl leading-relaxed mt-4 mb-4": {}
  },
  "p a": {
    "@apply text-foreground-900 underline font-medium": {}
  },
  "strong": {
    "@apply text-foreground-900 font-semibold": {}
  },
  "a strong": {
    "@apply text-inherit": {}
  },
  "blockquote strong": {
    "@apply text-inherit": {}
  },
  "thead th strong": {
    "@apply text-inherit": {}
  },
  "ol": {
    "@apply list-decimal mt-5 mb-5 pl-6": {}
  },
  "ol[type=\"A\"]": {
    "@apply list-[upper-alpha]": {}
  },
  "ol[type=\"a\"]": {
    "@apply list-[lower-alpha]": {}
  },
  "ol[type=\"A\" s]": {
    "@apply list-[upper-alpha]": {}
  },
  "ol[type=\"a\" s]": {
    "@apply list-[lower-alpha]": {}
  },
  "ol[type=\"I\"]": {
    "@apply list-[upper-roman]": {}
  },
  "ol[type=\"i\"]": {
    "@apply list-[lower-roman]": {}
  },
  "ol[type=\"I\" s]": {
    "@apply list-[upper-roman]": {}
  },
  "ol[type=\"i\" s]": {
    "@apply list-[lower-roman]": {}
  },
  "ol[type=\"1\"]": {
    "@apply list-decimal": {}
  },
  "ul": {
    "@apply list-disc mt-5 mb-5 pl-6": {}
  },
  "ol > li::marker": {
    "@apply font-normal text-foreground-500": {}
  },
  "ul > li::marker": {
    "@apply text-foreground-300": {}
  },
  "dt": {
    "@apply text-foreground-900 font-semibold mt-5": {}
  },
  "hr": {
    "@apply border-foreground-200 border-t mt-12 mb-12": {}
  },
  "blockquote": {
    "@apply font-medium italic text-foreground-900 border-l-4 border-l-foreground-200 mt-6 mb-6 pl-4": {},
    "quotes": "\"\\201C\"\"\\201D\"\"\\2018\"\"\\2019\""
  },
  "blockquote p:first-of-type::before": {
    "content": "open-quote"
  },
  "blockquote p:last-of-type::after": {
    "content": "close-quote"
  },
  "h1": {
    "@apply text-foreground-900 font-extrabold text-4xl mt-0 mb-3.5 leading-tight": {}
  },
  "h1 strong": {
    "@apply font-black text-inherit": {}
  },
  "h2": {
    "@apply text-foreground-900 font-bold text-2xl mt-8 mb-4 leading-snug": {}
  },
  "h2 strong": {
    "@apply font-extrabold text-inherit": {}
  },
  "h3": {
    "@apply text-foreground-900 font-semibold text-xl mt-6 mb-2 leading-relaxed": {}
  },
  "h3 strong": {
    "@apply font-bold text-inherit": {}
  },
  "h4": {
    "@apply text-foreground-900 font-semibold mt-6 mb-2 leading-normal": {}
  },
  "h4 strong": {
    "@apply font-bold text-inherit": {}
  },
  "img": {
    "@apply mt-8 mb-8": {}
  },
  "picture": {
    "@apply block mt-8 mb-8": {}
  },
  "video": {
    "@apply mt-8 mb-8": {}
  },
  "kbd": {
    "@apply font-medium text-foreground-900 text-sm rounded-md pt-0.5 pr-1.5 pb-0.5 pl-1.5": {},
    "fontFamily": "inherit"
  },
  "code": {
    "@apply text-foreground-900 font-semibold text-sm": {}
  },
  "a code": {
    "@apply text-inherit": {}
  },
  "h1 code": {
    "@apply text-inherit": {}
  },
  "h2 code": {
    "@apply text-inherit text-sm": {}
  },
  "h3 code": {
    "@apply text-inherit text-sm": {}
  },
  "h4 code": {
    "@apply text-inherit": {}
  },
  "blockquote code": {
    "@apply text-inherit": {}
  },
  "thead th code": {
    "@apply text-inherit": {}
  },
  "pre": {
    "@apply text-foreground-200 bg-surface-800 overflow-x-auto font-normal text-sm leading-loose mt-6 mb-6 rounded-md pt-3 pr-4 pb-3 pl-4": {}
  },
  "pre code": {
    "@apply bg-transparent border-0 rounded-none p-0 text-inherit": {},
    "fontWeight": "inherit",
    "fontSize": "inherit",
    "fontFamily": "inherit",
    "lineHeight": "inherit"
  },
  "pre code::before": {
    "content": "none"
  },
  "pre code::after": {
    "content": "none"
  },
  "table": {
    "@apply w-full table-auto text-left mt-8 mb-8 text-sm leading-loose": {}
  },
  "thead": {
    "@apply border-b border-b-foreground-300": {}
  },
  "thead th": {
    "@apply text-foreground-900 font-semibold align-bottom pr-2 pb-2 pl-2": {}
  },
  "tbody tr": {
    "@apply border-b border-b-foreground-200": {}
  },
  "tbody tr:last-child": {
    "@apply border-b-0": {}
  },
  "tbody td": {
    "@apply align-baseline": {}
  },
  "tfoot": {
    "@apply border-t border-t-foreground-300": {}
  },
  "tfoot td": {
    "@apply align-top": {}
  },
  "figure > *": {
    "@apply mt-0 mb-0": {}
  },
  "figcaption": {
    "@apply text-foreground-500 text-sm leading-normal mt-3": {}
  },
  "picture > img": {
    "@apply mt-0 mb-0": {}
  },
  "li": {
    "@apply mt-2 mb-2": {}
  },
  "ol > li": {
    "@apply pl-1.5": {}
  },
  "ul > li": {
    "@apply pl-1.5": {}
  },
  "> ul > li p": {
    "@apply mt-3 mb-3": {}
  },
  "> ul > li > *:first-child": {
    "@apply mt-5": {}
  },
  "> ul > li > *:last-child": {
    "@apply mb-5": {}
  },
  "> ol > li > *:first-child": {
    "@apply mt-5": {}
  },
  "> ol > li > *:last-child": {
    "@apply mb-5": {}
  },
  "ul ul, ul ol, ol ul, ol ol": {
    "@apply mt-3 mb-3": {}
  },
  "dl": {
    "@apply mt-5 mb-5": {}
  },
  "dd": {
    "@apply mt-2 pl-6": {}
  },
  "hr + *": {
    "@apply mt-0": {}
  },
  "h2 + *": {
    "@apply mt-0": {}
  },
  "h3 + *": {
    "@apply mt-0": {}
  },
  "h4 + *": {
    "@apply mt-0": {}
  },
  "thead th:first-child": {
    "@apply pl-0": {}
  },
  "thead th:last-child": {
    "@apply pr-0": {}
  },
  "tbody td, tfoot td": {
    "@apply pt-2 pr-2 pb-2 pl-2": {}
  },
  "tbody td:first-child, tfoot td:first-child": {
    "@apply pl-0": {}
  },
  "tbody td:last-child, tfoot td:last-child": {
    "@apply pr-0": {}
  },
  "figure": {
    "@apply mt-8 mb-8": {}
  }
}
